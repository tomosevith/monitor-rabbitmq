 [
    {
      "volumesFrom": [],
      "memory": ${memory},
      "portMappings": [
        {
          "containerPort": ${service_port},
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "${log_group_name}",
              "awslogs-stream-prefix": "${service_name}",
              "awslogs-region": "${region}"
          }
      },
      "essential": true,
      "entryPoint": ${entrypoint},
      "mountPoints": [],
      "name": "${service_name}",
      "dockerSecurityOptions": null,
      "environment": [
        {
          "name": "SSM_PROJECT_NAME",
          "value": "${ssm_project_name}"
        },
        {
          "name": "SSM_SERVICE_NAME",
          "value": "${ssm_service_name}"
        },
        {
          "name": "USE_AWS_PARAMETER_STORE",
          "value": "${use_aws_parameter_store}"
        },
        {
          "name": "AWS_REGION",
          "value": "${region}"
        }
      ],
      "links": null,
      "workingDirectory": null,
      "readonlyRootFilesystem": null,
      "image": "${image_url}",
      "command": ${command},
      "user": null,
      "dockerLabels": null,
      "cpu": 0,
      "privileged": null,
      "memoryReservation": null,
      "ulimits": [
        {
          "name": "nofile",
          "softLimit": 16384,
          "hardLimit": 16384
        }
      ]
    }
  ]
